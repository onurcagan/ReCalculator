<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Background">#1F1F1F</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>

    <Color x:Key="Contrast1">#9c61f3</Color>
    <SolidColorBrush x:Key="ContrastBrush" Color="{StaticResource Contrast1}"/>

    <Color x:Key="ForegroundWhite">White</Color>
    <SolidColorBrush x:Key="ForegroundWhiteBrush" Color="{StaticResource ForegroundWhite}"/>

    <Color x:Key="ForegroundGray">Gray</Color>
    <SolidColorBrush x:Key="ForegroundGrayBrush" Color="{StaticResource ForegroundGray}"/>

</ResourceDictionary>